package day2;

public class Book {
	private String bookTitle;
	private String author;
	private String ISBN;
	private int noofcopies;
	public String getBookTitle() {
		return bookTitle;
	}
	Book(String bookTitle, String author, String ISBN, int noofcopies){
		this.bookTitle=bookTitle;
		this.author=author;
		this.ISBN=ISBN;
		this.noofcopies=noofcopies;
	
		
	}
	private void display() {
		// TODO Auto-generated method stub
		System.out.println("Title:–"+ bookTitle +"Author:–" + author +"ISBN:–"+ ISBN +"Quantity:-"+noofcopies”);
	
	}
	public static void main(String args[]) {
		Book b = new Book("THE STORY OF MY EXPERIMENTS WITH TRUTH ","M.K.GANDHI","548755A5",10);
		b.display();
		
	}
	
	
	

}

