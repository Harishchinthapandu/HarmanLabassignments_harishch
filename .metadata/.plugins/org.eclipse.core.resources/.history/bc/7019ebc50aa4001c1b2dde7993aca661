package custom;

import java.util.Scanner;

public class Customer {
	 int id;
	    String name;
	    char gender;
	    String email;
	    String contactNumber;
	    Customer(int id,String name, char gender, String email,String contactNumber)
	    {
	    
	        this.id = id;
	        this.name =name;
	        this.gender= gender;
	        this.email=email;
	        this.contactNumber=contactNumber;
	    }
	    String getEmail(){
	        return email;
	    }
	     String getcontactNumber(){
	        return contactNumber;
	    }
}
public class C{
	 
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Customer1:");
        System.out.print("id:");
        int emp1_id=sc.nextInt();
        System.out.print("Name:");
        String emp1_name=sc.nextLine();
        System.out.print("Gender:");
        char emp1_gender=sc.next().charAt(0);
        System.out.print("email:");
        String emp1_email=sc.nextLine();
        System.out.print("contactNumber:");
        String emp1_contactNumber=sc.nextLine();
        System.out.println("Customer2:");
        System.out.print("id:");
        int emp2_id=sc.nextInt();
        System.out.print("Name:");
        String emp2_name=sc.nextLine();
        System.out.print("Gender:");
        char emp2_gender=sc.next().charAt(0);
        System.out.print("email:");
        String emp2_email=sc.nextLine();
        System.out.print("contactNumber:");
        String emp2_contactNumber=sc.nextLine();
 
 
        Customer cust1 = new Customer(emp1_id, emp1_name,emp1_gender,emp1_email,emp1_contactNumber);
        Customer cust2 = new Customer(emp2_id, emp2_name,emp2_gender,emp2_email,emp2_contactNumber);
       boolean b=cust1.getEmail().equals(cust1.getEmail());
       boolean c=cust2.getcontactNumber().equals(cust2.getcontactNumber());
        if(b==true && c==true){
            System.out.println("Customer 1 and Customer2 are same");
        }
        else{
            System.out.println("Customer 1 and Customer2 are different");
        }
    }
    }

